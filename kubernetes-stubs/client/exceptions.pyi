from typing import Any, Optional

class OpenApiException(Exception): ...

class ApiTypeError(OpenApiException, TypeError):
    path_to_item: Any = ...
    valid_classes: Any = ...
    key_type: Any = ...
    def __init__(self, msg: Any, path_to_item: Optional[Any] = ..., valid_classes: Optional[Any] = ..., key_type: Optional[Any] = ...) -> None: ...

class ApiValueError(OpenApiException, ValueError):
    path_to_item: Any = ...
    def __init__(self, msg: Any, path_to_item: Optional[Any] = ...) -> None: ...

class ApiKeyError(OpenApiException, KeyError):
    path_to_item: Any = ...
    def __init__(self, msg: Any, path_to_item: Optional[Any] = ...) -> None: ...

class ApiException(OpenApiException):
    status: Any = ...
    reason: Any = ...
    body: Any = ...
    headers: Any = ...
    def __init__(self, status: Optional[Any] = ..., reason: Optional[Any] = ..., http_resp: Optional[Any] = ...) -> None: ...

def render_path(path_to_item: Any): ...
